{
  "name": "Extension absent",
  "description": "Build an Extension for asent!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
  "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/absent_imgage.png"
    }
  }
}
